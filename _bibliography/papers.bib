address = {New York, NY, USA},
url = {https://doi.org/10.1145/3583678.3603278},
doi = {10.1145/3583678.3603278},
abstract = {In event processing systems, detected event patterns can reveal privacy-sensitive information. In this paper, we propose and discuss how to integrate pattern-level privacy protection in event-based systems. Compared to state-of-the-art approaches, we aim to enforce privacy independent of the particularities of specific operators. We accomplish this by supporting the flexible integration of multiple obfuscation techniques and studying deployment strategies for privacy-enforcing mechanisms. In addition, we share ideas on how to model the adversary's knowledge to select appropriate obfuscation techniques for the discussed deployment strategies. Initial results indicate that flexibly choosing obfuscation techniques and deployment strategies is essential to conceal privacy-sensitive event patterns accurately.},
booktitle = {Proceedings of the 17th ACM International Conference on Distributed and Event-Based Systems},
pages = {185–186},
numpages = {2},
keywords = {privacy, complex event processing, pattern},
location = {Neuchatel, Switzerland},
series = {DEBS '23}
}

@inproceedings{delouee2023,
title = "AQuA-CEP: Adaptive Quality-Aware Complex Event Processing in the Internet of Things",
abstract = "Sensory data profoundly influences the quality of detected events in a distributed complex event processing system (DCEP). Since each sensor{\textquoteright}s status is unstable at runtime, a single sensing assignment is often insufficient to fulfill the consumer{\textquoteright}s quality requirements. In this paper, we study in the context of AQuA-CEP the problem of dynamic quality monitoring and adaptation of complex event processing by active integration of suitable data sources. To support this, in AQuA-CEP, queries to detect complex events are supplemented with consumer-definable quality policies that are evaluated and used to autonomously select (or even configure) suitable data sources of the sensing infrastructure. In addition, we studied different forms of expressing quality policies and analyzed how it affects the quality monitoring process. Various modes of evaluating and applying quality-related adaptations and their impacts on correlation efficiency are addressed, too. We assessed the performance of AQuA-CEP in IoT scenarios by utilizing the notion of the quality policy alongside the query processing adaptation using knowledge derived from quality monitoring. The results show that AQuA-CEP can improve the performance of DCEP systems in terms of the quality of results while fulfilling the consumer{\textquoteright}s quality requirements. Quality-based adaptation can also increase the network{\textquoteright}s lifetime by optimizing the sensor{\textquoteright}s energy consumption due to efficient data source selection.",
keywords = "Complex Event Processing, Stream Processing, Adaptation, Quality, Internet of Things",
author = "{Lotfian Delouee}, Majid and Boris Koldehofe and Viktoriya Degeler",
year = "2023",
month = mar,
day = "14",
language = "English",
booktitle = "The 17th ACM International Conference on Distributed Event-Based Systems (DEBS 2023)",
publisher = "ACM Press",
note = "The 17th ACM International Conference on Distributed and Event-Based Systems (DEBS'23) ; Conference date: 27-06-2023 Through 30-06-2023",
url = "https://2023.debs.org/",
}

