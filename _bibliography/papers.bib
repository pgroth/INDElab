@inproceedings{10.1145/3583678.3603278,
author = {Lotfian Delouee, Majid and Koldehofe, Boris and Degeler, Viktoriya},
title = {Poster: Towards Pattern-Level Privacy Protection in Distributed Complex Event Processing},
year = {2023},
